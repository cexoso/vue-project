{
  "name": "@cexoso/sync-files",
  "version": "0.0.14",
  "description": "",
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false
  },
  "files": [
    "src",
    "!**/*.spec.ts",
    "!**/*.spec.tsx"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "repository": "https://github.com/cexoso/vue-project",
  "scripts": {
    "test": "vitest --reporter=dot",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "ts-lint": "tsc --noEmit",
    "release": "npm run ts-lint && vitest --run && npm run build",
    "prepublishOnly": "npm run release",
    "dev": "vite",
    "dev-server": "cross-env FILE_DIR=../temp-file tsx watch ./index.ts"
  },
  "sideEffects": false,
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cexoso/configs": "workspace: *",
    "@cexoso/ts-pkg-build": "^0.0.24",
    "@tailwindcss/vite": "^4.1.11",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^10.1.0",
    "koa-static": "^5.0.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.19.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue": "^3.5.22"
  },
  "dependencies": {
    "@cexoso/utils": "workspace: *",
    "koa": "^3.0.0",
    "koa-body": "^6.0.1",
    "koa-router": "^13.0.1"
  }
}
