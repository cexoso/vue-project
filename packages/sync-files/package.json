  "devDependencies": {
    "@cexoso/configs": "workspace: *",
    "@cexoso/ts-pkg-build": "^0.0.24",
    "@types/koa": "^2.15.0",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "tsx": "^4.19.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "koa": "^3.0.0"
